<h1>Listing bmet_models</h1>

<table>
  <thead>
    <tr>
      <th>Model name</th>
      <th>Manufacturer name</th>
      <th>Vendor name</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bmet_models.each do |bmet_model| %>
      <tr>
        <td><%= bmet_model.model_name %></td>
        <td><%= bmet_model.manufacturer_name %></td>
        <td><%= bmet_model.vendor_name %></td>
        <td><%= bmet_model.category %></td>
        <td><%= link_to 'Show', bmet_model %></td>
        <td><%= link_to 'Edit', edit_bmet_model_path(bmet_model) %></td>
        <td><%= link_to 'Destroy', bmet_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bmet model', new_bmet_model_path %>
