<%= simple_form_for(@bmet_model) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :model_name %>
    <%= f.input :manufacturer_name %>
    <%= f.input :vendor_name %>
    <%= f.input :category %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
