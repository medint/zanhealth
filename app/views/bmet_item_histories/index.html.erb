<h1>Listing bmet_item_histories</h1>

<table>
  <thead>
    <tr>
      <th>Bmet item</th>
      <th>Bmet item status</th>
      <th>Bmet item condition</th>
      <th>Remarks</th>
      <th>Work order</th>
      <th>Work order status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bmet_item_histories.each do |bmet_item_history| %>
      <tr>
        <td><%= bmet_item_history.bmet_item %></td>
        <td><%= bmet_item_history.bmet_item_status %></td>
        <td><%= bmet_item_history.bmet_item_condition %></td>
        <td><%= bmet_item_history.remarks %></td>
        <td><%= bmet_item_history.work_order %></td>
        <td><%= bmet_item_history.work_order_status %></td>
        <td><%= link_to 'Show', bmet_item_history %></td>
        <td><%= link_to 'Edit', edit_bmet_item_history_path(bmet_item_history) %></td>
        <td><%= link_to 'Destroy', bmet_item_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bmet item history', new_bmet_item_history_path %>
