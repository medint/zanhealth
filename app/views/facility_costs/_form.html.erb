<%= simple_form_for(@facility_cost) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :facility_cost_item_id %>
    <%= f.input :unit_quantity %>
    <%= f.input :cost %>
    <%= f.association :facility_work_order %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
