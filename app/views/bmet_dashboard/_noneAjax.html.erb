<%= simple_form_for :nothing, { url: url_for(:action => @action, :controller => 'bmet_dashboard'), method: 'get'} do |f| %>
	<div id="dashboard-dates">
		<%=t(:description).titlecase+': '+@description.titlecase%>
	</div>

	<div id="dashboard-submit-div">
	    <%= f.button :submit, "submit", :class => "dashboard-submit" %>
	</div>
<% end %>